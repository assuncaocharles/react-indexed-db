{
  "name": "react-indexed-db-hook",
  "version": "1.0.12",
  "description": "React wrapper to IndexedDB database.",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "src/*"
  ],
  "scripts": {
    "test": "karma start ./karma.conf.js",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "build": "webpack",
    "lint": "eslint src/**",
    "format": "prettier --write \"**/*.{js,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assuncaocharles/react-indexed-db.git"
  },
  "author": "Charles Assunção",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/assuncaocharles/react-indexed-db/issues"
  },
  "keywords": [
    "indexed",
    "db",
    "indexeddb",
    "websql",
    "sql",
    "indexed-db",
    "database",
    "react",
    "reactjs",
    "database",
    "storage"
  ],
  "homepage": "https://github.com/assuncaocharles/react-indexed-db",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.1",
    "typedoc": "^0.23.20",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
